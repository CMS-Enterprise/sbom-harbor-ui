{
  "name": "sbom-harbor-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-p 'lint:js' 'lint:other'",
    "lint:js": "eslint  --ext ts,tsx,js,jsx -c .eslintrc.cjs --no-eslintrc --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint --color  --report-unused-disable-directives --max-warnings 0",
    "lint:other": "prettier --check '**/*.{json,md,yaml,yml}' --ignore-path .gitignore --ignore-path .prettierignore",
    "fix": "run-p 'fix:js' 'fix:other'",
    "fix:js": "yarn run lint:js --fix",
    "fix:other": "yarn run lint:other --write",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.5.1",
  "engines": {
    "node": "19",
    "npm": "9.6.5",
    "yarn": "3.5.1"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "yarn lint:js",
    "**/{json,md,yaml,yml}": "yarn lint:other"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "@mui/system": "^5.12.1",
    "@mui/x-data-grid": "^5.17.26",
    "@popperjs/core": "^2.11.7",
    "aws-amplify": "^5.1.4",
    "classnames": "^2.3.2",
    "clipboard-copy": "^4.0.1",
    "lodash": "^4.17.21",
    "mdi-material-ui": "^7.7.0",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.11.0",
    "uuid": "^9.0.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.56",
    "@swc/jest": "^0.2.26",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/uuid": "^9.0.1",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.3.2",
    "web-vitals": "^3.3.1"
  }
}
